<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>

	</ui:style>
	<g:HTMLPanel>
		<div id="login_box">
			Username: <g:TextBox ui:field="username"></g:TextBox><br/>
			Password: <g:PasswordTextBox ui:field="password"></g:PasswordTextBox>
			<g:SimplePanel ui:field="submit"></g:SimplePanel><br/><br/>
			<g:SimplePanel ui:field="register"></g:SimplePanel>
		</div>
	</g:HTMLPanel>
</ui:UiBinder> 